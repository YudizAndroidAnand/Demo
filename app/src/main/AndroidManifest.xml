<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31" >
        <activity
            android:name=".DialogBoxActivity"
            android:exported="false" />
        <activity
            android:name=".IntentFilterActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/*" />
                <data android:mimeType="image/*" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomNotificationView"
            android:exported="false" />
        <activity
            android:name=".PassingData"
            android:exported="false" />
        <activity
            android:name=".Layout.SetLayout"
            android:exported="false" />
        <activity
            android:name=".Layout.LayoutFrame"
            android:exported="false" />
        <activity
            android:name=".Layout.LayoutRelative"
            android:exported="false" />
        <activity
            android:name=".Layout.LayoutLinear"
            android:exported="false" />
        <activity
            android:name=".Layout.LayoutConstraint"
            android:exported="false" />
        <activity
            android:name=".ActivityRuntimePermission"
            android:exported="false" />
        <activity
            android:name=".ActivityLayout"
            android:exported="false" />
        <activity
            android:name=".ActivityView"
            android:exported="false" />
        <activity
            android:name=".ActivityFragment"
            android:exported="false" />
        <activity
            android:name=".CustomButton"
            android:exported="false" />
        <activity
            android:name=".Shapesdrawable"
            android:exported="false" />
        <activity
            android:name=".Tab_layout"
            android:exported="false" />
        <activity
            android:name=".RecycleView.RecyclerView"
            android:exported="false" />
        <activity
            android:name=".Floatingbutton"
            android:exported="false" />
        <activity
            android:name=".ActivityNotification"
            android:exported="false" />
        <activity
            android:name=".ActivityAllproject"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>