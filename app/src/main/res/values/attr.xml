<?xml version="1.0" encoding="utf-8"?>
<resources>
 <declare-styleable name= "ButtonProgressBar">

  <attr name="text" format="string"/>
  <attr name="enable" format="boolean"/>
  <attr name="bgcolor" format="color"/>
  <attr name="custextcolor" format="reference|color"/>
  <attr name="size" format="string">

   <enum name ="Default" value = "0"></enum>
